<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <managed-bean>
        <managed-bean-name>beanIndex</managed-bean-name>
        <managed-bean-class>beans.beanIndex</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>beanBlog</managed-bean-name>
        <managed-bean-class>beans.beanBlog</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>beanActividades</managed-bean-name>
        <managed-bean-class>beans.beanActividades</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>beanContacto</managed-bean-name>
        <managed-bean-class>beans.beanContacto</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>manageSesion</managed-bean-name>
        <managed-bean-class>beans.manageSesion</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>beanAsesoramiento</managed-bean-name>
        <managed-bean-class>beans.beanAsesoramiento</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>beanInsertar</managed-bean-name>
        <managed-bean-class>beans.beanInsertar</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>beanBorrar</managed-bean-name>
        <managed-bean-class>beans.beanBorrar</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>beanModificar</managed-bean-name>
        <managed-bean-class>beans.beanModificar</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
</faces-config>
